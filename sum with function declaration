#include<stdio.h>
int main(){
    int a,b;
    printf("enter the first number\n");
    scanf("%d",&a);
    printf("enter the second number");
    scanf("%d",&b);

    int result=sum(a,b);
    printf("the result is %d",result);
 return 0;
}
int sum(int num1,int num2){
 int result=num1+num2;
 return result;

}
